<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="Android Submediant video Intercom demo program \nНастраивать\n"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:textStyle="normal|bold"
            android:textSize="20sp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=" Receive output  frame ：" />

            <RadioGroup
                android:orientation="vertical"
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:id="@+id/UseLnkLstRadioBtn"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:checked="false"
                    android:text="Linked list" />

                <RadioButton
                    android:id="@+id/UseAjbRadioBtn"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:checked="true"
                    android:text=" Self-designed Adaptive jitter buffer" />

            </RadioGroup>

            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:enabled="false"
                    android:visibility="invisible"/>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickAjbSetting" />

            </LinearLayout>

        </LinearLayout>

        <CheckBox
            android:id="@+id/IsUseFrgndSrvcCheckBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text=" Use front desk services" />

        <CheckBox
            android:id="@+id/IsSaveSettingToFileCheckBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text=" Save Настраивать To the extension directory Setting.txt File " />

        <CheckBox
            android:id="@+id/IsPrintLogcatCheckBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text=" print LogcatLog" />

        <CheckBox
            android:id="@+id/IsUseWakeLockCheckBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text=" Use wake lock " />

        <TextView
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="normal|bold"
            android:textSize="20sp"
            android:text="\nsoundvideo Preset Настраивать\n" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text=" Effect level ：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text=" low "
                    android:id="@+id/UseEffectLowRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseEffectLowRadioBtn"/>

                <RadioButton
                    android:text="in"
                    android:id="@+id/UseEffectMidRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseEffectMidRadioBtn"/>

                <RadioButton
                    android:text="high"
                    android:id="@+id/UseEffectHighRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseEffectHighRadioBtn"/>

                <RadioButton
                    android:text="ultra"
                    android:id="@+id/UseEffectSuperRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseEffectSuperRadioBtn"/>

                <RadioButton
                    android:text="special"
                    android:id="@+id/UseEffectPremiumRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseEffectPremiumRadioBtn"/>

            </RadioGroup>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text="ratiospecial Rate grade ：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text=" low "
                    android:id="@+id/UseBitrateLowRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseBitrateLowRadioBtn"/>

                <RadioButton
                    android:text="in"
                    android:id="@+id/UseBitrateMidRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseBitrateMidRadioBtn"/>

                <RadioButton
                    android:text="high"
                    android:id="@+id/UseBitrateHighRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseBitrateHighRadioBtn"/>

                <RadioButton
                    android:text="ultra"
                    android:id="@+id/UseBitrateSuperRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseBitrateSuperRadioBtn"/>

                <RadioButton
                    android:text="special"
                    android:id="@+id/UseBitratePremiumRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:onClick="OnClickUseBitratePremiumRadioBtn"/>

            </RadioGroup>

        </LinearLayout>

        <TextView
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="normal|bold"
            android:textSize="20sp"
            android:text="\n Audio Настраивать\n" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text=" Sampling frequency ：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text="8000"
                    android:id="@+id/UseAudioSamplingRate8000RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="16000"
                    android:id="@+id/UseAudioSamplingRate16000RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="32000"
                    android:id="@+id/UseAudioSamplingRate32000RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

            </RadioGroup>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text=" frame  Millisecond length ：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text="10ms"
                    android:id="@+id/UseAudioFrameLen10msRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="20ms"
                    android:id="@+id/UseAudioFrameLen20msRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="30ms"
                    android:id="@+id/UseAudioFrameLen30msRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

            </RadioGroup>

        </LinearLayout>

        <CheckBox
            android:id="@+id/IsUseSystemAecNsAgcCheckBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" Use the acoustic feedback that comes with the system sound Canceller, noise sound Suppressor and automatic gain controller （ The system does not necessarily come with it ）"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:text=" Acoustic echo sound Eliminator ："
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"/>

            <RadioGroup
                android:orientation="vertical"
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:id="@+id/UseNoAecRadioBtn"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text=" Do not use " />

                <RadioButton
                    android:id="@+id/UseSpeexAecRadioBtn"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="Speex Acoustic echo sound Eliminator " />

                <RadioButton
                    android:id="@+id/UseWebRtcAecmRadioBtn"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="WebRtc Fixed-point version  Acoustic echo sound Eliminator " />

                <RadioButton
                    android:text="WebRtc Floating point version  Acoustic echo sound Eliminator "
                    android:id="@+id/UseWebRtcAecRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"/>

                <RadioButton
                    android:id="@+id/UseSpeexWebRtcAecRadioBtn"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="SpeexWebRtc triple  Acoustic echo sound Eliminator " />

            </RadioGroup>

            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:enabled="false"
                    android:visibility="invisible"/>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickSpeexAecSetting"/>

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:onClick="OnClickWebRtcAecmSetting"/>

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:onClick="OnClickWebRtcAecSetting"/>

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:onClick="OnClickSpeexWebRtcAecSetting"/>

            </LinearLayout>

        </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:text=" noise sound Suppressor ："
                    android:gravity="center_vertical"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioGroup
                    android:orientation="vertical"
                    android:layout_weight="7"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">

                    <RadioButton
                        android:text=" Do not use "
                        android:id="@+id/UseNoNsRadioBtn"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"/>

                    <RadioButton
                        android:id="@+id/UseSpeexPprocNsRadioBtn"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="Speex Preprocessor  noise sound торможение " />

                    <RadioButton
                        android:text="WebRtc Fixed-point version  noise sound Suppressor "
                        android:id="@+id/UseWebRtcNsxRadioBtn"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"/>

                    <RadioButton
                        android:text="WebRtc Floating point version  noise sound Suppressor "
                        android:id="@+id/UseWebRtcNsRadioBtn"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"/>

                    <RadioButton
                        android:id="@+id/UseRNNoiseRadioBtn"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="RNNoise noise sound Suppressor " />

                </RadioGroup>

            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:enabled="false"
                    android:visibility="invisible"/>

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:onClick="OnClickSpeexPprocNsSetting" />

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickWebRtcNsxSetting"/>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickWebRtcNsSetting" />

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:enabled="false"
                    android:visibility="invisible"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <CheckBox
                android:id="@+id/IsUseSpeexPprocOtherCheckBox"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=" use Speex Preprocessor  Other functions "/>

            <Button
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Настраивать"
                android:onClick="OnClickSpeexPprocOtherSetting"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=" Codec ：" />

            <RadioGroup
                android:orientation="vertical"
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:id="@+id/UsePcmRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:text="PCM Raw data "/>

                <RadioButton
                    android:id="@+id/UseSpeexCodecRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:text="Speex Codec "/>

                <RadioButton
                    android:id="@+id/UseOpusCodecRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:text="Opus Codec "
                    android:enabled="false"/>

            </RadioGroup>

            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:enabled="false"
                    android:visibility="invisible"/>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickSpeexCodecSetting" />

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickOpusCodecSetting"
                    android:enabled="false"/>

            </LinearLayout>

        </LinearLayout>

        <CheckBox
            android:id="@+id/IsSaveAudioToFileCheckBox"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text=" will  Audio  enter  frame 、 Audio  Output  frame 、 Audio  result  frame  Replace separately  Save  To the extension directory AudioInput.wav、AudioOutput.wav、AudioResult.wav File " />

        <TextView
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="normal|bold"
            android:textSize="20sp"
            android:text="\nvideoНастраивать\n" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text=" Sampling frequency ：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text="12"
                    android:id="@+id/UseVideoSamplingRate12RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="16"
                    android:id="@+id/UseVideoSamplingRate16RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="24"
                    android:id="@+id/UseVideoSamplingRate24RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="30"
                    android:id="@+id/UseVideoSamplingRate30RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

            </RadioGroup>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text=" frame 大小：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text="144*176"
                    android:id="@+id/UseVideoFrameSize144_176RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="240*320"
                    android:id="@+id/UseVideoFrameSize240_320RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="480*640"
                    android:id="@+id/UseVideoFrameSize480_640RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="960*1280"
                    android:id="@+id/UseVideoFrameSize960_1280RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

            </RadioGroup>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text=" Display zoom ：" />

            <RadioGroup
                android:orientation="horizontal"
                android:layout_weight="9"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:text=" No zoom"
                    android:id="@+id/UseDisplayScale1_0RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="1.5 Times"
                    android:id="@+id/UseDisplayScale1_5RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="2 Times"
                    android:id="@+id/UseDisplayScale2_0RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

                <RadioButton
                    android:text="3 Times"
                    android:id="@+id/UseDisplayScale3_0RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"/>

            </RadioGroup>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:gravity="center_vertical"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=" Codec ：" />

            <RadioGroup
                android:orientation="vertical"
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <RadioButton
                    android:id="@+id/UseYU12RadioBtn"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:text="YU12 Raw data "/>

                <RadioButton
                    android:id="@+id/UseOpenH264CodecRadioBtn"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:text="OpenH264 Codec "/>

            </RadioGroup>

            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <Button
                    android:text="Настраивать"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:enabled="false"
                    android:visibility="invisible"/>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Настраивать"
                    android:onClick="OnClickOpenH264CodecSetting" />

            </LinearLayout>

        </LinearLayout>

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text=" determine "
            android:onClick="OnClickSettingOk" />

    </LinearLayout>

</ScrollView>